    "cors": "^2.7.1",
    "express": "^4.13.3",
    "lodash": "^4.17.21",
    "morgan": "^1.8.0",
    "resource-router-middleware": "^0.6.0",
    "web3": "^1.0.0-beta.55"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
